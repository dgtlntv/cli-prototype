{
    "$schema": "./commands-schema.json",
    "welcome": "Welcome to the Enhanced CLI Prototype!\nType 'help' to get a list of all available commands.\n",
    "variables": {
        "username": "unset",
        "isLoggedIn": "false"
    },
    "commands": {
        "register": {
            "handler": [
                {
                    "component": "text",
                    "output": "Registering in progress...",
                    "duration": 2000
                },
                {
                    "component": "text",
                    "output": "Registered successfully"
                }
            ]
        }
    }
}
